<p id="notice"><%= notice %></p>

<h1>Listing Medicos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Data nascimento</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Crm</th>
      <th>Especialidade</th>
      <th>Local trabalho</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicos.each do |medico| %>
      <tr>
        <td><%= medico.nome %></td>
        <td><%= medico.cpf %></td>
        <td><%= medico.email %></td>
        <td><%= medico.data_nascimento %></td>
        <td><%= medico.endereco %></td>
        <td><%= medico.bairro %></td>
        <td><%= medico.cidade %></td>
        <td><%= medico.estado %></td>
        <td><%= medico.crm %></td>
        <td><%= medico.especialidade %></td>
        <td><%= medico.local_trabalho %></td>
        <td><%= link_to 'Show', medico %></td>
        <td><%= link_to 'Edit', edit_medico_path(medico) %></td>
        <td><%= link_to 'Destroy', medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medico', new_medico_path %>
