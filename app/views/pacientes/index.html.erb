<p id="notice"><%= notice %></p>

<h1>Listing Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Data nascimento</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Plano de saude</th>
      <th>Num registro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.nome %></td>
        <td><%= paciente.cpf %></td>
        <td><%= paciente.email %></td>
        <td><%= paciente.data_nascimento %></td>
        <td><%= paciente.endereco %></td>
        <td><%= paciente.bairro %></td>
        <td><%= paciente.cidade %></td>
        <td><%= paciente.estado %></td>
        <td><%= paciente.plano_de_saude %></td>
        <td><%= paciente.num_registro %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>
